<div id="column_dimension" class="tab_content">
  <!-- Column Dimension -->
  <table>
    <tr>
      <td colspan="9"><label class="sub_title">SECTION SUMMARY</label></td>
    </tr>
    <tr>
      <td><b>No</b></td>
      <td><b>Description</b></td>
      <td><b>Tray Type</b></td>
      <td><b>T.Count</b></td>
      <td><b>T.Spacing</b></td>
      <td><b>HETP</b></td>
      <td><b>Height</b></td>
      <td><b>Diameter</b></td>
      <td><b>Section &Delta;P</b></td>
    </tr>
    <tr>
      <td>1</td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
