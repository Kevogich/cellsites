<div id="system_definition" class="tab_content">
  <!-- System Definition -->
  <div style="border: 1px solid #000000;">
    <div id="system_definition_image">
      <div id="process_basis_div">
        <ul class="unstyled">
          <li>
            <label style="font-weight: bold;text-decoration: underline;">Process Basis</label>
          </li>
          <li>
            <%= f.collection_select :sd_process_basis_id, @company.heat_and_material_balances, :id, :case, :prompt => true %>
          </li>
        </ul>
      </div>

      <div id="streams_1" class="streams">
        <ul class="unstyled">
          <li><%= f.collection_select :sd_stream_no_1, @streams, :stream_no, :display_stream_no, {:include_blank => ""}, {:style => "width:100px;", :class => "sd_stream_no", :sno => 1} %></li>
          <li><%= f.text_field :sd_pressure_1, :class => "pressure_general", :style => "width:60px;" %>&nbsp;<label class="pressure_general_unit"></label>
          </li>
          <li><%= f.text_field :sd_temperature_1, :class => "temperature_general", :style => "width:60px;" %>&nbsp;<label class="temperature_general_unit"></label>
          </li>
        </ul>
      </div>

      <div id="streams_2" class="streams">
        <ul class="unstyled">
          <li><%= f.collection_select :sd_stream_no_2, @streams, :stream_no, :display_stream_no, {:include_blank => ""}, {:style => "width:100px;", :class => "sd_stream_no", :sno => 2} %></li>
          <li><%= f.text_field :sd_pressure_2, :class => "pressure_general required", :style => "width:60px;" %>&nbsp;<label class="pressure_general_unit"></label>
          </li>
          <li><%= f.text_field :sd_temperature_2, :class => "temperature_general", :style => "width:60px;" %>&nbsp;<label class="temperature_general_unit"></label>
          </li>
        </ul>
      </div>

      <div id="streams_3" class="streams">
        <ul class="unstyled">
          <li><%= f.collection_select :sd_stream_no_3, @streams, :stream_no, :display_stream_no, {:include_blank => ""}, {:style => "width:100px;", :class => "sd_stream_no", :sno => 3} %></li>
          <li><%= f.text_field :sd_pressure_3, :class => "pressure_general", :style => "width:60px;" %>&nbsp;<label class="pressure_general_unit"></label>
          </li>
          <li><%= f.text_field :sd_temperature_3, :class => "temperature_general", :style => "width:60px;" %>&nbsp;<label class="temperature_general_unit"></label>
          </li>
        </ul>
      </div>

      <div id="streams_4" class="streams">
        <ul class="unstyled">
          <li><%= f.collection_select :sd_stream_no_4, @streams, :stream_no, :display_stream_no, {:include_blank => ""}, {:style => "width:100px;", :class => "sd_stream_no", :sno => 4} %></li>
          <li><%= f.text_field :sd_pressure_4, :class => "pressure_general required", :style => "width:60px;" %>&nbsp;<label class="pressure_general_unit"></label>
          </li>
          <li><%= f.text_field :sd_temperature_4, :class => "temperature_general", :style => "width:60px;" %>&nbsp;<label class="temperature_general_unit"></label>
          </li>
        </ul>
      </div>

      <div id="streams_5" class="streams">
        <ul class="unstyled">
          <li><%= f.collection_select :sd_stream_no_5, @streams, :stream_no, :display_stream_no, {:include_blank => ""}, {:style => "width:100px;", :class => "sd_stream_no", :sno => 5} %></li>
          <li><%= f.text_field :sd_pressure_5, :class => "pressure_general", :style => "width:60px;" %>&nbsp;<label class="pressure_general_unit"></label>
          </li>
          <li><%= f.text_field :sd_temperature_5, :class => "temperature_general", :style => "width:60px;" %>&nbsp;<label class="temperature_general_unit"></label>
          </li>
        </ul>
      </div>

      <div id="streams_6" class="streams">
        <ul class="unstyled">
          <li><%= f.collection_select :sd_stream_no_6, @streams, :stream_no, :display_stream_no, {:include_blank => ""}, {:style => "width:100px;", :class => "sd_stream_no", :sno => 6} %></li>
          <li><%= f.text_field :sd_pressure_6, :class => "pressure_general", :style => "width:60px;" %>&nbsp;<label class="pressure_general_unit"></label>
          </li>
          <li><%= f.text_field :sd_temperature_6, :class => "temperature_general", :style => "width:60px;" %>&nbsp;<label class="temperature_general_unit"></label>
          </li>
        </ul>
      </div>

      <div id="btn_condenser_type" style="position:absolute;top:5px;left:420px;">
        <input type="button" value="&nbsp;" style="background:#30FF10;">&nbsp;
        <strong>Condenser
          Type:<label id="sd_condenser_type"><%= f.label column_sizing.sd_cont_main_condenser_type %></label></strong>
      </div>

      <div id="btn_column_type" style="position:absolute;top:180px;left:225px;">
        <input type="button" value="&nbsp;" style="background:#30FF10;">&nbsp;
        <strong>Column Type:<label id="sd_column_type"><%= column_sizing.sd_column_type %></label></strong>
        <%= f.hidden_field :sd_column_type %>
