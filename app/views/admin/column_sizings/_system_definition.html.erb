    </div>

    <div id="condenser_type_div">
      <table>
        <tr>
          <td><label style="font-weight: bold;">Condenser Outlet Temperature</label></td>
          <td><label id="condenser_outlet_temperature" style="font-weight: bold;"></label></td>
        </tr>
        <tr>
          <td>
            <fieldset class="form_fieldset">
              <legend>Cooling Medium</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_cont_cooling_medium, "air" %>&nbsp;Air</li>
                <li><%= f.radio_button :sd_cont_cooling_medium, "cooling_water" %>&nbsp;Cooling Water</li>
                <li><%= f.radio_button :sd_cont_cooling_medium, "refrigerant" %>&nbsp;Refrigerant</li>
              </ul>
            </fieldset>
          </td>
          <td rowspan="3">
            <input type="button" value="Enable All"><br><br>
            <input type="button" value="Close" onclick="$.colorbox.close();">
          </td>
        </tr>
        <tr>
          <td>
            <fieldset class="form_fieldset">
              <legend>Main Condenser Type</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_cont_main_condenser_type, "aerial_condenser" %>&nbsp;Aerial Condenser</li>
                <li><%= f.radio_button :sd_cont_main_condenser_type, "shell_n_tube_exchanger" %>&nbsp;Shell & Tube
                  Exchanger
                </li>
              </ul>
            </fieldset>
          </td>
        </tr>
        <tr>
          <td>
            <fieldset class="form_fieldset">
              <legend>Trim Condensing</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_cont_trim_condensing, "yes" %>&nbsp;Yes</li>
                <li><%= f.radio_button :sd_cont_trim_condensing, "no" %>&nbsp;No</li>
              </ul>
            </fieldset>
          </td>
        </tr>
      </table>
    </div>
  </div>

  <script type="text/javascript">

    <% if column_sizing.new_record?%>
    $("input[name='column_sizing[sd_rt_available_reboiler_types]']").attr('disabled', 'disabled');
    <% else%>
    <% end%>

    $('#btn_reboiler_type').click(function () {
      $.colorbox({
        href:"#reboiler_type_div",
        inline:true,
        onClosed:function () {
          var reboiler_type = $("input[name='column_sizing[sd_rt_available_reboiler_types]']:checked").parent('li').text();
          $('label#sd_reboiler_type').text(reboiler_type);
        }
      });
    });

    $('#btn_column_type').click(function () {
      $.colorbox({
        href:"#column_type_div",
        inline:true,
        onClosed:function () {
          var column_type = $("input[name='column_sizing[sd_ct_column_type_selected]']:checked").parent('li').text();
          $('label#sd_column_type').text(column_type);
          $('#column_sizing_sd_column_type').val(column_type);
        }
      });
    });

    $('#btn_condenser_type').click(function () {
      $.colorbox({
        href:"#condenser_type_div",
        inline:true,
        onOpen:function () {
          var outlet_temp = $('#column_sizing_sd_temperature_5').val();
          if (outlet_temp != '') {
            $('#condenser_outlet_temperature').text(outlet_temp);

            if (outlet_temp >= 125) {
              $('#column_sizing_sd_cont_cooling_medium_air').attr("checked", "checked");
              $('#column_sizing_sd_cont_main_condenser_type_aerial_condenser').attr("checked", "checked");
              $('#sd_condenser_type').text("Aerial Condenser");
            } else if (outlet_temp >= 95 && outlet_temp < 125) {
              $('#column_sizing_sd_cont_cooling_medium_cooling_water').attr("checked", "checked");
              $('#column_sizing_sd_cont_main_condenser_type_shell_n_tube_exchanger').attr("checked", "checked");
              $('#sd_condenser_type').text("Shell & Tube Exchanger");
            } else if (outlet_temp < 95) {
              $('#column_sizing_sd_cont_cooling_medium_refrigerant').attr("checked", "checked");
              $('#column_sizing_sd_cont_main_condenser_type_shell_n_tube_exchanger').attr("checked", "checked");
              $('#sd_condenser_type').text("Shell & Tube Exchanger");
            }
          } else {
            alert('No condensate temperature is entered for the column system.  Please enter a condensate temperature.');
            $.colorbox.close();
            return false;
          }
        },
        onClosed:function () {
          var condenser_type = $("input[name='column_sizing[sd_cont_main_condenser_type]']:checked").parent('li').text();
          $('label#sd_condenser_type').text(condenser_type);
        }
      });
    });

    $("input[name='column_sizing[sd_cont_cooling_medium]']").change(function () {
      if ($(this).val() == "air") {
        $("#column_sizing_sd_cont_main_condenser_type_aerial_condenser").attr('checked', "checked");
        $("#column_sizing_sd_cont_trim_condensing_yes").attr('checked', "checked");
      } else if ($(this).val() == "cooling_water") {
        $("#column_sizing_sd_cont_main_condenser_type_shell_n_tube_exchanger").attr('checked', "checked");
        $("#column_sizing_sd_cont_trim_condensing_yes").attr('checked', false);
      } else if ($(this).val() == "refrigerant") {
        $("#column_sizing_sd_cont_main_condenser_type_shell_n_tube_exchanger").attr('checked', "checked");
        $("#column_sizing_sd_cont_trim_condensing_yes").attr('checked', false);
      } else {
        $("#column_sizing_sd_cont_main_condenser_type_aerial_condenser").attr('checked', false);
        $("#column_sizing_sd_cont_main_condenser_type_shell_n_tube_exchanger").attr('checked', false);
        $("#column_sizing_sd_cont_trim_condensing_yes").attr('checked', false);
      }
    });

    $('#reboiler_type_enable_all').click(function () {
      $("input[name='column_sizing[sd_rt_available_reboiler_types]']").attr('disabled', false);
    });

    $("#column_sizing_sd_rt_fluid_characteristics_clean_service").change(function () {
      if ($(this).is(':checked')) {
        $("#column_sizing_sd_rt_extent_of_fouling_heavy_fouling").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_extent_of_fouling_moderate_fouling").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_pressure_less_than_atmospheric").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_pressure_more_than_atmospheric").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_viscosity_high_viscosity").attr('disabled', false);
        $("#column_sizing_sd_rt_viscosity_moderate_viscosity").attr('disabled', false);
        $("#column_sizing_sd_rt_area_required_large").attr('disabled', false);
        $("#column_sizing_sd_rt_area_required_moderate").attr('disabled', false);
      }
    });

    $("#column_sizing_sd_rt_fluid_characteristics_fouling_service").change(function () {
      if ($(this).is(':checked')) {
        $("#column_sizing_sd_rt_extent_of_fouling_heavy_fouling").attr('disabled', false);
        $("#column_sizing_sd_rt_extent_of_fouling_moderate_fouling").attr('disabled', false);
        $("#column_sizing_sd_rt_pressure_less_than_atmospheric").attr('disabled', false);
        $("#column_sizing_sd_rt_pressure_more_than_atmospheric").attr('disabled', false);
        $("#column_sizing_sd_rt_viscosity_high_viscosity").attr('disabled', false);
        $("#column_sizing_sd_rt_viscosity_moderate_viscosity").attr('disabled', false);
        $("#column_sizing_sd_rt_area_required_large").attr('disabled', false);
        $("#column_sizing_sd_rt_area_required_moderate").attr('disabled', false);
      }
    });

    $("#column_sizing_sd_rt_extent_of_fouling_heavy_fouling").change(function () {
      if ($(this).is(':checked')) {
        $("#column_sizing_sd_rt_available_reboiler_types_pump_through_reboiler").attr('disabled', false);
        $("#column_sizing_sd_rt_available_reboiler_types_kettle_reboiler").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_vertical_thermosiphon").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_horizontal_thermosiphon").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_internal_reboiler").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_viscosity_moderate_viscosity").attr('disabled', false);
        $("#column_sizing_sd_rt_pressure_less_than_atmospheric").attr('disabled', false);
        $("#column_sizing_sd_rt_pressure_more_than_atmospheric").attr('disabled', false);
      }
    });

    $("#column_sizing_sd_rt_viscosity_high_viscosity").change(function () {
      if ($(this).is(':checked')) {
        $("#column_sizing_sd_rt_area_required_large").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_area_required_moderate").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_pump_through_reboiler").attr('disabled', false);
        $("#column_sizing_sd_rt_available_reboiler_types_kettle_reboiler").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_vertical_thermosiphon").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_horizontal_thermosiphon").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_internal_reboiler").attr('disabled', 'disabled');
      }
    });

    $("#column_sizing_sd_rt_area_required_large").change(function () {
      if ($(this).is(':checked')) {
        $("#column_sizing_sd_rt_available_reboiler_types_pump_through_reboiler").attr('disabled', false);
        $("#column_sizing_sd_rt_available_reboiler_types_kettle_reboiler").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_vertical_thermosiphon").attr('disabled', 'disabled');
        $("#column_sizing_sd_rt_available_reboiler_types_horizontal_thermosiphon").attr('disabled', false);
        $("#column_sizing_sd_rt_available_reboiler_types_internal_reboiler").attr('disabled', 'disabled');
      }
    });

    $("#column_sizing_sd_rt_pressure_less_than_atmospheric").change(function () {
      if ($(this).is(':checked')) {
