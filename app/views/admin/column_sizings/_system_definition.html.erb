        <tr>
          <td colspan="2" align="center">
            <fieldset class="form_fieldset">
              <legend>Fluid Characteristics</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_rt_fluid_characteristics, "fouling_service" %>&nbsp;Fouling Service</li>
                <li><%= f.radio_button :sd_rt_fluid_characteristics, "clean_service" %>&nbsp;Clean Service</li>
              </ul>
            </fieldset>
          </td>
        </tr>
        <tr>
          <td>
            <fieldset class="form_fieldset">
              <legend>Extent of Fouling</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_rt_extent_of_fouling, "heavy_fouling" %>&nbsp;Heavy Fouling</li>
                <li><%= f.radio_button :sd_rt_extent_of_fouling, "moderate_fouling" %>&nbsp;Moderate Fouling</li>
              </ul>
            </fieldset>
          </td>
          <td>
            <fieldset class="form_fieldset">
              <legend>Viscosity</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_rt_viscosity, "high_viscosity" %>&nbsp;Heavy Viscosity</li>
                <li><%= f.radio_button :sd_rt_viscosity, "moderate_viscosity" %>&nbsp;Moderate Viscosity</li>
              </ul>
            </fieldset>
          </td>
        </tr>
        <tr>
          <td>
            <fieldset class="form_fieldset">
              <legend>Pressure</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_rt_pressure, "less_than_atmospheric" %>&nbsp;Less Than Atmospheric</li>
                <li><%= f.radio_button :sd_rt_pressure, "more_than_atmospheric" %>&nbsp;More Than Atmospheric</li>
              </ul>
            </fieldset>
          </td>
          <td>
            <fieldset class="form_fieldset">
              <legend>Area Required</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_rt_area_required, "large" %>&nbsp;Large</li>
                <li><%= f.radio_button :sd_rt_area_required, "moderate" %>&nbsp;Moderate</li>
              </ul>
            </fieldset>
          </td>
        </tr>
        <tr>
          <td>
            <fieldset class="form_fieldset">
              <legend>Available Reboiler Types</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_rt_available_reboiler_types, "pump_through_reboiler" %>&nbsp;Pump Through
                  Reboiler
                </li>
                <li><%= f.radio_button :sd_rt_available_reboiler_types, "kettle_reboiler" %>&nbsp;Kettle Reboiler</li>
                <li><%= f.radio_button :sd_rt_available_reboiler_types, "horizontal_thermosiphon" %>&nbsp;Horizontal
                  Thermosiphon
                </li>
                <li><%= f.radio_button :sd_rt_available_reboiler_types, "vertical_thermosiphon" %>&nbsp;Vertical
                  Thermosiphon
                </li>
                <li><%= f.radio_button :sd_rt_available_reboiler_types, "internal_reboiler" %>&nbsp;Internal Reboiler</li>
              </ul>
            </fieldset>
          </td>
          <td style="text-align: center;vertical-align: middle;">
            <input type="button" value="Enable All" id="reboiler_type_enable_all"><br><br>
            <input type="button" value="Close" onclick="$.colorbox.close();">
          </td>
        </tr>
      </table>
    </div>

    <div id="column_type_div">
      <table>
        <tr>
          <td><label class="sub_title">CONSIDERATION FOR TRAY TOWER DESIGN SELECTION</label></td>
          <td rowspan="15" style="padding: 10px;">
            <fieldset class="form_fieldset">
              <legend>Column Type Selected</legend>
              <ul class="unstyled">
                <li><%= f.radio_button :sd_ct_column_type_selected, "tray" %>Tray</li>
                <li><%= f.radio_button :sd_ct_column_type_selected, "packed_bed" %>Packed Bed</li>
                <li><%= f.radio_button :sd_ct_column_type_selected, "tray_n_packing_combo" %>Tray & Packing Combo</li>
              </ul>
            </fieldset>
          </td>
        </tr>
        <tr>
          <td><%= f.check_box :sd_ct_chk_box_1 %>&nbsp;Components consists of solids & foulants</td>
        </tr>
        <tr>
          <td><%= f.check_box :sd_ct_chk_box_2 %>&nbsp;There are many internal transitions</td>
        </tr>
        <tr>
          <td><%= f.check_box :sd_ct_chk_box_3 %>&nbsp;Liquid load is high</td>
        </tr>
        <tr>
          <td><%= f.check_box :sd_ct_chk_box_4 %>&nbsp;Lack of experience with the service</td>
        </tr>
        <tr>
          <td><%= f.check_box :sd_ct_chk_box_5 %>&nbsp;Vessel wall need periodic inspection</td>
        </tr>
        <tr>
          <td><%= f.check_box :sd_ct_chk_box_6 %>&nbsp;There are multiple liquid phases</td>
        </tr>
