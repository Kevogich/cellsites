  <table>
    <tr>
      <td colspan="4"><label class="sub_title">COLUMN SPECIFICATIONS</label></td>
    </tr>
    <tr>
      <td>Minimum Column Stages</td>
      <td><%= f.text_field :sd_minimum_column_stages %></td>
      <td>No. of Trays Above Feed</td>
      <td><%= f.text_field :sd_no_of_trays_above_feed %></td>
    </tr>
    <tr>
      <td>Minimum Reflux Ratio</td>
      <td><%= f.text_field :sd_minimum_reflux_ratio %></td>
      <td>No. of Trays Below Feed</td>
      <td><%= f.text_field :sd_no_of_trays_below_feed %></td>
    </tr>
    <tr>
      <td>Optimal Reflux Ratio Factor</td>
      <td><%= f.text_field :sd_optimal_reflux_ratio_factor %></td>
      <td>Overall Tray Efficiency</td>
      <td><%= f.text_field :sd_overall_tray_efficiency %>&nbsp;%</td>
    </tr>
    <tr>
      <td>Actual Reflux Ratio</td>
      <td><%= f.text_field :sd_actual_reflux_ratio %></td>
      <td>Total Actual Tray</td>
      <td><%= f.text_field :sd_total_actual_tray %></td>
    </tr>
    <tr>
      <td>Optimal Stage Ratio</td>
      <td><%= f.text_field :sd_optimal_stage_ratio %></td>
      <td>Foaming Service</td>
      <td><%= f.select :sd_foaming_service, StaticData.foaming_service.collect{|p| [raw(p), p]}, :include_blank => true%></td>
    </tr>
    <tr>
      <td>No of Theoretical Column Trays</td>
      <td><%= f.text_field :sd_no_of_theoretical_column_trays %></td>
      <td>System Factor</td>
      <td><%= f.text_field :sd_system_factor %></td>
    </tr>
  </table>

  <!-- reboiler_type_div -->
  <div class="hidden">
    <div id="reboiler_type_div">
