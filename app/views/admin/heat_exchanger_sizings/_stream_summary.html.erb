    <tr>
      <td>Vapor Specific Heat Capacity</td>
      <td><%= f.text_field :ss_hi_vapor_specific_heat_capacity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_vapor_specific_heat_capacity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_vapor_specific_heat_capacity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_vapor_specific_heat_capacity, :class=>"small-text-100" %></td>
      <td>kJ/kg &deg;K</td>
    </tr>
    <tr>
      <td>Vapor Thermal Conductivity</td>
      <td><%= f.text_field :ss_hi_vapor_thermal_conductivity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_vapor_thermal_conductivity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_vapor_thermal_conductivity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_vapor_thermal_conductivity, :class=>"small-text-100" %></td>
      <td>W/m &deg;K</td>
