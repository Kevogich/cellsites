    </tr>
    <tr>
      <td>Liquid Expansion Coefficient</td>
      <td><%= f.text_field :ss_hi_liquid_expansion_coefficient, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_liquid_expansion_coefficient, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_liquid_expansion_coefficient, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_liquid_expansion_coefficient, :class=>"small-text-100" %></td>
      <td>1/&deg;C</td>
    </tr>
    <tr>
      <td>Liquid BubblePoint Temperature</td>
      <td><%= f.text_field :ss_hi_liquid_bubblepoint_temperature, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_liquid_bubblepoint_temperature, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_liquid_bubblepoint_temperature, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_liquid_bubblepoint_temperature, :class=>"small-text-100" %></td>
      <td>&deg;C</td>
    </tr>
    <tr>
      <td>Vapor Flow Rate</td>
      <td><%= f.text_field :ss_hi_vapor_flow_rate, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_vapor_flow_rate, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_vapor_flow_rate, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_vapor_flow_rate, :class=>"small-text-100" %></td>
      <td>kg/hr</td>
    </tr>
    <tr>
      <td>Vapor MW</td>
      <td><%= f.text_field :ss_hi_vapor_mw, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_vapor_mw, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_vapor_mw, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_vapor_mw, :class=>"small-text-100" %></td>
