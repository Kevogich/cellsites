      <td><%= f.text_field :ss_ho_vapor_viscosity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_vapor_viscosity, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_vapor_viscosity, :class=>"small-text-100" %></td>
      <td>cP</td>
