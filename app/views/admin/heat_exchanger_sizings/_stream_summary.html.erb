      <td><%= f.text_field :ss_hi_vapor_k, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ho_vapor_k, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_ci_vapor_k, :class=>"small-text-100" %></td>
      <td><%= f.text_field :ss_co_vapor_k, :class=>"small-text-100" %></td>
