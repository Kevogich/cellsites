<div class="colorbox-dialog">
<%= form_for :group, :url => admin_groups_path, :remote => true do |f| %>
	<%= f.error_messages%>
	<div class="form-element-last">
		<%= f.label :name%><br>
		<%= f.text_field :name%>
	</div>
	<div class="form-buttons">
		<%= f.submit "Create" %>
  	</div>
<% end%>
</div>  